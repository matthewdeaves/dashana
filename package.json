{
  "name": "dashana",
  "version": "1.0.0",
  "description": "Asana CSV to static report website",
  "scripts": {
    "dev": "eleventy --serve",
    "build": "eleventy",
    "test": "jest --runInBand --testPathIgnorePatterns=integration",
    "test:watch": "jest --watch --testPathIgnorePatterns=integration",
    "test:integration": "TEST_TYPE=integration jest tests/integration.test.js",
    "test:coverage": "jest --coverage --testPathIgnorePatterns=integration",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "validate:html": "html-validate \"_site/**/*.html\""
  },
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "csv-parse": "^6.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "cheerio": "^1.1.2",
    "html-validate": "^10.5.0",
    "jest": "^30.2.0"
  }
}
